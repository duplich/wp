/* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
@import "breadcrump";


// title of the page
#title {
    
    @extend %col_base_size;
    margin-top: $s_middle;

    h1, h2 {
        //font-size: $f_size_19;
    }

    h3, h4 {
        text-transform: none;
        color: $c_logo_grey;
        font-weight: normal;
        font-size: $f_size_18;
    }

    h1, h2, h3, h4 {
        margin-top: 0px;
        margin-bottom: 0;
        //margin: $t_margin*0.2 0;
    }

}


// header image
#haederimg {

    @extend %col_full_size;

}





