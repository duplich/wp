    html,
    button,
    input,
    select,
    textarea {
        color: #222;
    }

    body {
        background-color: $c_bg;
        font-family: $f_sistony;
        font-size: $f_size_base;
        line-height: $lh_standard;
        min-width: $width_standard;
        
    }

    /*
     * Remove text-shadow in selection highlight: h5bp.com/i
     * These selection rule sets have to be separate.
     * Customize the background color to match your design.
     */

    ::-moz-selection {
        background: #b3d4fc;
        text-shadow: none;
    }

    ::selection {
        background: #b3d4fc;
        text-shadow: none;
    }

    /*
     * A better looking default horizontal rule
     */

    hr {
        display: block;
        height: 1px;
        border: 0;
        border-top: 1px solid #ccc;
        margin: 1em 0;
        padding: 0;
    }

    /*
     * Remove the gap between images and the bottom of their containers: h5bp.com/i/440
     */

    img {
        vertical-align: middle;
    }

    /*
     * Remove default fieldset styles.
     */

    fieldset {
        border: 0;
        margin: 0;
        padding: 0;
    }

    /*
     * Allow only vertical resizing of textareas.
     */

    textarea {
        resize: vertical;
    }

    /* 
     *  Chrome Frame prompt
     */

    .chromeframe {
        margin: 0.2em 0;
        background: #ccc;
        color: #000;
        padding: 0.2em 0;
    }



/* 1.2 - FORMS AND INPUTS */
/* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
input, textarea {

    border: $border_slight;
    background-color: $c_grey_light_ultra;
    border-radius: 1px;
    width: 100%;
    padding: .5em;
    @include border_box;
    min-height: 40px;
    margin-bottom: $t_margin;

}

textarea {
    min-height: 40px*5;
}

input[type="submit"] {
    width: 100%;
    border: none;
}

label {
    //color: $c_grey_middle;
    font-weight: bold;
    text-transform: uppercase;
}




/* 1.3 - TYPOGRAPHY */
/* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */

h1 {
    @include f_headline( $c_black, bold);
    font-size: $f_size_36; 
}

h2 {
    @include f_headline( $c_black, bold);
    font-size: $f_size_28; 
}

h3 {
    @include f_headline( $c_black, bold);
    font-size: $f_size_24; 
}

h4 {

    @include f_headline( $c_black, bold);
    font-size: $f_size_18; 

}

h5 {
    @include f_headline( $c_black, bold);
    font-size: $f_size_16; 
}

h6 {
    @include f_headline( $c_black, bold);
    font-size: $f_size_14; 
}



h1, h2, h3, h4, h5, h6 {
    line-height: $lh_heading;
    margin: $t_margin 0;
}



// passsage handling
p {

    @include f_text ($c_base_font_color);
    margin-top: $t_margin;
    margin-bottom: $t_margin;
    

    // last pessage has greater margin
    &:last-of-type {
      margin-bottom: $t_margin_passage; 
    }


    // large marign when <h1-6> follows <p>
    &+h1, &+h2, &+h3, &+h4, &+h5, &+h6 {
        margin-top: $t_margin_passage;
    }

}


// zitate
blockquote {

    @include f_text ($c_grey_dark);
    font-size: 1.143em; // 16px
    margin: $t_margin_passage $t_margin_passage*1.5;
    text-align: center;


    &:before {
        content: "Â» ";
    }

    &:after {
        content: " Â«";
    }
}


    



/* 1.4 - ICONS */
