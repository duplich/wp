/* 3.1 - TOPBAR */
/* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */

#topbar {
    
    @extend %col_full_size;

    display: block;
    position: relative;
   
    z-index: 90;

    // list base settings
    ul {
        margin: 0;
        padding: 0;
    }

    ul li {
        list-style: none;
    }

    h4, h5 {
        font-size: $f_size_14;
    }
}// end topbar





// logobar - first row fllowed by nav-row
#logobar {

    ul li {
        display: inline-block;
    }

    height: $topbar_height;
    margin: 0 $span_40;

    // cotnains the logo
    a.logo h1,
    a.logo h2 {

        background-image: url( ../img/reusable/logo_bewegungsloft_fuerth.png );
        float: left;
        height: $logo_height; 
        margin: $logo_distance_top 0 0 0;
        width: $logo_width;  

    }


    a {
        @extend %a-grey-light;
    }


    // additional link section for social links ...
    .links {

        float: right;

        ul.contactlinks,
        ul.sociallinks {

            float: left;
            margin: $logobar_links_top_margin 0 0 0;
            text-transform: uppercase;

            .icon:before {
                //icons
                font-size: 1.4em;
                padding-right: 4px;
                vertical-align: -3px;
            }
        }

        ul.contactlinks {
            li {
                margin-left: 32px;
            }
        }

        ul.sociallinks {
            margin-left: 50px;
            li {
                margin-left: 8px;
            }
            
        }

    }
    
}// end #logobar
















