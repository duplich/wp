/* 3.4 - FOOTER */
/* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */

@import "footer";






























/* 3.5 - MODULES */
/* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
    
    
    /*%base-frame {
        margin: $s_middle $span_frame 0 $span_frame;
    }*/


    /* 3.5.1 - MODULES - Contactblock */
    /* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
    .contactblock {
        
        @extend %col_base_size;
        margin-top: $s_middle;

        .button {
            margin-bottom: $s_half;
        }

    }
   




    /* 3.5.2 - MODULES - Catchwords */
    /* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */

    .catchwords {

        @extend %col_base_size;

        .modul {

            //padding: 0.6em 0;

            h4 {
                font-size: $f_size_13;
                //margin: 0.3em 0;
            }

            p {
                //margin: 0.3em 0;
            }

        }        

    }


    /* 3.5.3 - MODULES - Article */
    /* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
    .article {

        @extend %col_base_size;


        .modul {

            margin-top: $s_middle;

            h1, h2, h3, h4, h5, h6, p, blockquote {
                padding: 0 2px;
            }

            img {
                width: 100%;
                margin: $t_margin 0;
            }

            .besmaller img {
                width: 99%;
                padding: 0 0.5%;
            }

            .subline {
                font-size: $f_size_18;
                color: $c_logo_grey;
                font-weight: normal;
            }
            

        }

        .headline-near {
            &+.modul {
                margin-top: $s_third;
            }
            h1, h2, h3, h4, h5, h6 {
                margin: 0 0 $s_third 0;
            }
        }


        .headline-border {
            border-bottom: $border_std;

            &+.modul {
                margin-top: $s_base;
            }
        }



        .headline-free {

            &+.modul {
                margin-top: $s_base;
            }
        }



        .serviceslisting {

            padding: $s_base $s_near $s_plus $s_near;
            background-color: $c_grey_light;
            display: inline-block;

            .headline {
                margin-top: 0px;
            }

            .imgcontainer {
               
                width: 100%;
                height: 160px;
                overflow: hidden;
                margin: 0 0 $s_half 0;
                position: relative;

                img {
                    margin: 0px;
                }

                .overlay {
                    height: 160px;
                    width: 100%;
                    background-color: $c_white;
                    opacity: 0;
                    position: absolute;
                    @include transition( opacity, .4s );

                    .notice {
                        padding: 10% 5%;
                    
                        p {
                            color: $c_logo_grey;
                            margin: 0 0 $s_third 0;
                        }
                    }
                }
            }

            a:hover {

                .overlay {
                    top: 0px;
                    opacity: .9;
                }

                h5 {
                    color: $c_logo_red;
                }
                
            }

            h5 {
                //text-transform: none;
                //color: $c_grey_dark;
                font-size: 15px;
                //font-weight: normal;
                margin: 0;
                //height: $s_middle;
            }

            .quater {

                //border-bottom: 2px solid $c_stone_light;
            }
        }

    }



    /* 3.5.4 - MODULES - Tagarea */
    /* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
    .tagarea {

        @extend %col_base_size;
        border-top: $border_std;
        padding: $s_near 0;
        margin-top: $s_middle;

        .modul {

            h4, div {
                font-size: $f_size_13;
                display: inline;
            }

            h4 {
                font-weight: bold;
            }
            
        }
        
    }



    /* 3.5.5 - MODULES - Linkbox */
    /* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
    .linkboxes {
        
        @extend %col_full_size;
        margin-top: $s_middle;

    }

    .linkbox {
        background-color: $c_white;
        

        .content {
            height: 160px;
            margin: $s_half;
            position: relative;

            p, h5 {
                text-align: center;
            }

            h5 {
                font-size: $f_size_24;
                margin-top: 0;
            }

            p {
                margin-bottom: 0;
                padding: 0 8%;
            }

            .buttonwrapper {

                text-align: center;
                position: absolute;
                bottom: 0px;
                width: 100%;

                .button,
                .textbutton {
                    display: inline-block;
                }
                
                .textbutton {
                    @extend %a-grey-middle;
                    margin-bottom: 8px;
                }
                .textbutton .btnbody:before {
                    color: $c_black;
                }
                
            }

        }
    }
    








    /* 3.5.6 - MODULES - Comments */
    /* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
    #commentarea {
        
        background-color: $c_white;
        padding: $s_middle 0;
        margin-top: $s_middle;
    
        .wrapper {
            @extend %col_base_size;
        }

        &+#footer {
            margin-top: 0px;
        }
        
    }

    .commentform {

        .comment-submit {
            margin: $t_margin_passage 0;
        }

        .comment-info {
            margin: $t_margin 0;
        }
    }



    .comment {

        border-bottom: $border_std;
        border-bottom-color: $c_bg;

        
        .name {
            margin-top: $t_margin_passage;
        }

        .tagline {
            color: $c_grey_dark;
            font-size: $f_size_13;
        }
    }
    



    /* 3.5.7 - MODULES - foursteps */
    /* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
    .foursteps {
        
        @extend %col_base_size;
        background: url(../img/reusable/bewegungsloft_trainingsraum.jpg) no-repeat;
        height: 430px;
        margin-top: $s_middle;

        
        .headline, .content {
            position: relative;
            top: 45px;
        }
        
        .headline {
            
            display: block;
            width: 1090px;
            

            h4 {
                padding: 4px 30px;
                display: inline-block;
                font-size: $f_size_28; 
                background-color: $c_white;
            }
        }



        .content {
            background-color: $c_white;
            width: 1030px;
            padding: 30px;

            p {
                margin-bottom: 0;
                padding-right: 5px;
            }

            .bar {
                height: 38px;
                background: url(../img/reusable/4_schritte_in_das_personal_training.png) no-repeat;
                margin-bottom: $s_half;
                position: relative;

                .button {
                    cursor: pointer;
                    display: block;
                    width: 28px;
                    height: 28px;
                    color: $c_white;
                    font-size: 28px;
                    position: absolute;
                    
                    &:hover {
                        color: $c_logo_red;
                    }
                }
            }
        }

    }































