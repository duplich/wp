/* 2.2 - GRID SYSTEM */
/* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */

.wrap-full,
 %col_full_size {
    width: $width_standard;
    margin-left: auto;
    margin-right: auto;
 }  

.wrap-base,
 %col_base_size {
    width: $width_standard - $span_40*2;
    margin-left: auto;
    margin-right: auto;
 } 





%base-division {

    display: block;
    //float: left;
    margin-right: $span_gutter;

    &.last {
        margin-right: 0px;
    }
}


.full {
    @extend %base-division;
    width: $span_full - $span_gutter;
}


.half {
    @extend %base-division;
    width: $span_halft;
}


.third {
    @extend %base-division;
    width: $span_third;
}

    .twothirds {
        @extend %base-division;
        width: $span_third * 2 + $span_gutter;
    }


.quater {
    @extend %base-division;
    width: $span_quater;
}

    .threequaters {
        @extend %base-division;
        width: $span_quater * 3 + $span_gutter*2;
    }


.fifth {
    @extend %base-division;
    width: $span_fifth;
}

    .twofifths {
        @extend %base-division;
        width: $span_fifth*2 + $span_gutter;
    }

    .threefifths {
        @extend %base-division;
        width: $span_fifth*3 + $span_gutter*2;
    }

    .fourfifths {
        @extend %base-division;
        width: $span_fifth*4 + $span_gutter*3;
    }
    




// has to be after the span-sections for overriding purpose
.left {
    float: left;
}

.right {
    float: right;
}




// margins top
.mt-third { margin-top: $s_third; }
.mt-half { margin-top: $s_half; }
.mt-near { margin-top: $s_near; }
.mt-base { margin-top: $s_base; }
.mt-plus { margin-top: $s_plus; }
.mt-middle { margin-top: $s_middle; }
.mt-sized { margin-top: $s_sized; }
.mt-big { margin-top: $s_big; }
.mt-huge { margin-top: $s_huge; }

// margin centre
.mc { margin-left: auto; margin-right: auto; }










/* 2.3 - LISTS */
/* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */


ul, ol, dl {
    margin: $t_margin_passage 0px;
    list-style: square;
}





















