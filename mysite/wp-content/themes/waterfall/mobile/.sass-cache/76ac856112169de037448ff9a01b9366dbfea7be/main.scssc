/* 3.5 - MODULES */
/* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */
    
    
    %base-frame {
        margin: $s_middle $span_frame 0 $span_frame;
    }


    /* 3.5.1 - MODULES - Quick Contact */
    /* :::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::::: */

    .quickcontact {

        margin-top: $s_third;
        padding-top: $s_third;

        //-------------> begin tabs   
        section.tabs {

            a.tab {

                color: $c_stone_black;
                cursor: pointer;
                display: block;
                float: left;
                font-size: $f_size_13;
                text-align: center;
                text-decoration: none;
                text-transform: uppercase;
                width: 33%;

                &.active {
                    background-color: $c_black;
                    color: $c_white;

                     div.icon {
                        color: $c_white;
                     }
                }

                &:active {
                    background-color: $c_logo_red;
                    color: $c_white;

                     div.icon {
                        color: $c_white;
                     }
                }



                div.icon {

                    color: $c_logo_red;
                    font-size: 23px;
                    height: 35px;
                    padding-top: 5px;
                    width: 100%;

                }
                div.description {
                    height: 30px;
                }

                // call icon-font
                &.wayto div.icon:after {
                    @include set_icon ("place");
                    
                }

                &.callus div.icon:after {
                    @include set_icon ("phone");
                }

                &.writeus div.icon:after {
                    @include set_icon ("letter");
                }

